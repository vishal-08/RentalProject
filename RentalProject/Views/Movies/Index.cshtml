@model IEnumerable<RentalProject.Models.Movie>
@{
    {
        ViewBag.Title = "Index";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }

    <h2>Movies</h2>

    <table class="table table-striped">
        <thead>
            <tr>
                <th scope="col">Movie</th>

            </tr>
        </thead>
        <tbody>

            @foreach (var movie in Model)
            {
            <tr>
                <th scope="row">@movie.Name</th>
            </tr>  
                    }

                
            </tbody>
    </table>
}


